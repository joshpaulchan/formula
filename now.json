{
    "version": 2,
    "name": "formula",
    "builds": [{
        "src": "formula/wsgi.py",
        "use": "@ardnt/now-python-wsgi",
        "config": {
            "maxLambdaSize": "5mb"
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "formula/wsgi.py"
    }],
    "env": {
        "DB_USER": "formula-db-user",
        "DB_PASSWORD": "formula-db-password",
        "DB_NAME": "formula-db-name",
        "DB_HOST": "formula-db-host"
    }
}